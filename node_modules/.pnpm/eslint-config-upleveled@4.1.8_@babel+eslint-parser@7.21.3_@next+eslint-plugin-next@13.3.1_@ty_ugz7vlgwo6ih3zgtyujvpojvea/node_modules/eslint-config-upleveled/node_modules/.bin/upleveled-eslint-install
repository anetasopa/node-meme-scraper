#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sopa/projectsNext/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@4.1.8_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.1_@ty_ugz7vlgwo6ih3zgtyujvpojvea/node_modules/eslint-config-upleveled/bin/node_modules:/Users/sopa/projectsNext/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@4.1.8_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.1_@ty_ugz7vlgwo6ih3zgtyujvpojvea/node_modules/eslint-config-upleveled/node_modules:/Users/sopa/projectsNext/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@4.1.8_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.1_@ty_ugz7vlgwo6ih3zgtyujvpojvea/node_modules:/Users/sopa/projectsNext/node-meme-scraper/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sopa/projectsNext/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@4.1.8_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.1_@ty_ugz7vlgwo6ih3zgtyujvpojvea/node_modules/eslint-config-upleveled/bin/node_modules:/Users/sopa/projectsNext/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@4.1.8_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.1_@ty_ugz7vlgwo6ih3zgtyujvpojvea/node_modules/eslint-config-upleveled/node_modules:/Users/sopa/projectsNext/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@4.1.8_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.1_@ty_ugz7vlgwo6ih3zgtyujvpojvea/node_modules:/Users/sopa/projectsNext/node-meme-scraper/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/install.js" "$@"
else
  exec node  "$basedir/../../bin/install.js" "$@"
fi
